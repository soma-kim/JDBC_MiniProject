	</entry>
	
	<entry key="updateProduct">
	UPDATE PRODUCT
	SET PRODUCT_NAME = ?
	  , PRICE = ?
	  , DESCRIPTION = ?
	  , STOCK = ?
	WHERE PRODUCT_ID = ?
	</entry>
	
