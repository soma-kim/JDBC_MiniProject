<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>SQL</comment>
	
	<entry key="selectAll">
	SELECT PRODUCT_ID
	     , PRODUCT_NAME
	     , PRICE
	     , DESCRIPTION
	     , STOCK
	FROM PRODUCT
	</entry>
	
	<entry key="insertProduct">
	INSERT INTO PRODUCT
	VALUES (?, ?, ?, ?, ?)

	</entry>
	
